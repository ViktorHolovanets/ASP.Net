@page
@model MyWebShop.Pages.ShowCategoriesModel
@{
    
}
<div class="content">
    @foreach (var p in @Model.GetCategory())
    {
        <div class="product">
            <img src="@p.Image"/>
            <p class="nameProduct">@p.Name</p>
            <p class="priceProduct">@p.Price ₴</p>
            <input type="button" value="+" class="addBasket"/>
        </div>
    }
   
</div>
