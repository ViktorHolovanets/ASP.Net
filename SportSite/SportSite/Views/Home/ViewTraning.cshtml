@model IEnumerable<Training>
<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-3 bg-light text-black">
            @item.training<hr />
            @foreach (var day in item.dayofWeeks)
            {
                <p>@day.dayofWeek @day.Time</p>
            }
        </div>
    }
</div>

