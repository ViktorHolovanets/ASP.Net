@model User
<div class="user">
    @if (Model != null)
    {
        @using (Html.BeginForm("EditUser", "Home", FormMethod.Post))
        {
            @Html.EditorForModel()
            <input type="submit" value="Save"/>
        }
        
    }
    else
    {
        <h2>Is not find user</h2>
    }
</div>
